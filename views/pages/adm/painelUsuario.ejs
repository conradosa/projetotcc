    %>
      <div class="row col-sm-10">
        <table class="table table-stripped">
          <thead>
            <tr>
              <th>Matricula</th>
              <th>Nome</th>
              <th>Email</th>
              <th>---</th>
            </tr>
          </thead>
          <tbody>
            <% for(i=0;i < Object.keys(usuario).length; i++){ %>
            <tr>
              <td><a href="#"> <%= usuario[i].matricula;%></a> </td>
              <td><a href="#"> <%= usuario[i].nome;  %> </a></td>
              <td><a href="#"> <%= usuario[i].email; %> </a></td>
            <% } %>
          </tbody>
        </table>
      </div>
    <% } %>
      
  </div>
